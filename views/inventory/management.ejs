<% if (title) { %>
    <h1>
        <%= title %>
    </h1>
<% } else { res.redirect('/') } %>

<% if (errors) { %>
    <div class="notice">
   <% errors.array().forEach(error => { %>
     <p><%= error.msg %></p>
  <%  }) %>
   </div>
<% } %>

<% if (messages && messages.length > 0) { %>
    <div class="notice">
      <% messages.forEach(message => { %>
        <p><%= message %></p>
      <% }) %>
    </div>
  <% } %>

<div class="list flex column">
    <a class="text-left" href="/inv/add-classification">Add New Classification</a>
    <a class="text-left" href="/inv/add-inventory">Add New Vehicle</a>
</div>